	//},
	"go.goplsOptions": {
		"usePlaceholders": true,
		"linksInHover": true,
		"hoverKind": "FullDocumentation"
	},
	"suggest.completionItemKindLabels": {
		"function": "",
		"method": "⚙️ ",
		"variable": "⚗️",
		"constant": "",
		"struct": " ",
