{
	"coc.source.around.firstMatch": true,
	"codeLens.enable": false,
	"diagnostic.virtualText": true,
	"diagnostic.errorSign": "⛔",
	"diagnostic.warningSign": "🟠",
	"diagnostic.infoSign": "🟢",
	"diagnostic.enableMessage": "jump",
	"diagnostic.enableHighlightLineNumber": true,
	"diagnostic.hintSign": "🔵",
	"codeLens.separator": "  ",
	"codeLens.subseparator": ">>>>",
	"coc.source.issues.priority": 99,
	"signature.preferShownAbove": false,
	"coc.preferences.jumpCommand": "drop",
	"coc.preferences.formatOnSaveFiletypes": ["go","typescript","typescriptreact", "svelte", "js", "javascript", "ts", "rust", "vim", "dart"],
	"suggest.enablePreview": false,
	"suggest.detailField": "preview",
	"suggest.noselect": true,
	"suggest.removeDuplicateItems": true,
	"suggest.numberSelect": false,
	"coc.source.buffer.priority": 1,
	"list.source.outline.defaultArgs": ["-kind"],
	

	"svelte.plugin.typescript.definitions.enable": true,
	
